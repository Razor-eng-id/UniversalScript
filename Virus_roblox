-- [[ MODUL PENGHANCUR FISIK OBBY ]] --
task.spawn(function()
    while task.wait(5) do -- Setiap 5 detik, cek objek baru
        for _, part in pairs(workspace:GetDescendants()) do
            if part:IsA("BasePart") and part.Name:lower():find("touch") or part.Name:lower():find("kill") then
                pcall(function()
                    part.Size = Vector3.new(50, 1, 50) -- Memperbesar jebakan agar semua orang mati
                    part.Transparency = 0.5
                    part.Color = Color3.new(1, 0, 0)
                end)
            end
        end
    end
end)
